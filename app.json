{
    "expo": {
        "name": "Minut'oeuf",
        "slug": "minutoeuf",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icons.png",
        "splash": {
            "image": "./assets/splashs.png",
            "resizeMode": "contain",
            "backgroundColor": "#f1cd50"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#FFFFFF"
            }
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}
